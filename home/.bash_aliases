
# copy to 'clipboard'
alias cb="xclip -selection clipboard"
# list direcotires
alias ldir="ls .*/ */ -d"
# tmux attach alias
alias tma='tmux attach -t'
# alias to look for a process
alias psg="ps -ef | grep -v grep | grep"
# alias to launch ipython with math profile
alias ipymath="ipython --profile=math"
